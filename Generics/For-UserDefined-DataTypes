//Student Class

public class Student {
    private String name;
    private int age;

    public Student(String name, int age) {
        this.name = name;
        this.age = age;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getAge() {
        return age;
    }

    public void setAge(int age) {
        this.age = age;
    }

    @Override
    public String toString() {
        return "Student{" +
                "name='" + name + '\'' +
                ", age=" + age +
                '}';
    }
}

//Generic Class of Student Type
public class GenericClassObject<T> {
    private T value;

    public GenericClassObject(T value) {
        this.value = value;
    }

    public T getValue() {
        return value;
    }

    public void setValue(T value) {
        this.value = value;
    }

    @Override
    public String toString() {
        return "GenericClassObject{" +
                "value=" + value +
                '}';
    }

    public static void main(String[] args) {   

        //Passing Student Object of type Student;

        GenericClassObject<Student> studentObject = new GenericClassObject<>(new Student("Alice", 20));
        System.out.println(studentObject);
    }
}
